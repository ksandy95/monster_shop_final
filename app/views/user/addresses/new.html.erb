<h1>New Address Form</h1>
<%= form_for @address, url: create_user_address_path(@user, @address) do |f| %>
  <%= f.label :address %>
  <%= f.text_field :address %>

  <%= f.label :city %>
  <%= f.text_field :city %>

  <%= f.label :state %>
  <%= f.text_field :state %>

  <%= f.label :zip %>
  <%= f.text_field :zip %>

  <%= f.submit %>

<% end %>
